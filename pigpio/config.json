{
  "name": "pigpio",
  "version": "1.2.0",
  "slug": "pigpio",
  "description": "Control your pwm leds with pigpio on Hass.io",
  "url": "https://github.com/Poeschl/Hassio-Addons/tree/master/pigpio",
  "arch": [
    "armhf",
    "armv7"
  ],
  "startup": "services",
  "boot": "auto",
  "gpio": true,
  "privileged": ["SYS_RAWIO"],
  "devices":["/dev/mem", "/dev/vcio"],
  "image": "ghcr.io/poeschl/ha-pigpio-{arch}",
  "ports": {
    "8888/tcp": "8888"
  },
  "schema": {},
  "options": {}
}
